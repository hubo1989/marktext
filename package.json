{
  "name": "marktext-next",
  "version": "0.18.3",
  "description": "MarkText Next - Next Generation Markdown Editor",
  "main": "./out/main/index.js",
  "author": "Marktext Contributors (Tkaixiang fork)",
  "homepage": "https://github.com/hubo1989/marktext-next",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --publish never",
    "build:mac": "npm run build && electron-builder --mac --publish never",
    "build:linux": "npm run build && electron-builder --linux --publish never",
    "build:release": "npm run build && electron-builder --win --mac --linux --publish never",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint:fix": "eslint --cache --fix .",
    "format:check": "prettier --check .",
    "code:check": "npm run lint && npm run format:check",
    "code:fix": "npm run lint:fix && npm run format",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
